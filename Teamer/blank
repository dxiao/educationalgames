// find all permutations of str and add the given prefix (use "")
public static void permute(String str, String prefix) {
    int n = str.length();
    if (n == 0) {
        System.out.println(prefix);
    } else {
        for (int i = 0; i < n; i++) {
            permute(str.substring(0, i) + str.substring(i+1, n), prefix + str.charAt(i));
        }
    }
}
